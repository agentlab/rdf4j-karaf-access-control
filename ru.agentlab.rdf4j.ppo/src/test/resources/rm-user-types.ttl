#####################################
# Типы атрибутов
#####################################
rmUserTypes:primaryText
  a rdf:Property ;
  dcterms:title "Primary Text" .

rmUserTypes:primaryTextShape
  sh:name               "primaryText" ;
  sh:path               rmUserTypes:primaryText ;
  sh:maxCount           1 ;
  sh:datatype           xsd:string .


#####################################
# Форматы
#####################################
rmUserTypes:ArtifactFormats
  a rdf:type .

rmUserTypes:_YwcOsRmREemK5LEaKhoOow
  a rmUserTypes:ArtifactFormats ;
  rm:enumEntries [
    a rdf:Seq ;
    rdf:_1 rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Module ;
    rdf:_2 rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Collection ;
    rdf:_3 rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Text
  ] ;
  dc:description "" ;
  dc:title "Форматы артефактов" ;
  rm:valueType xsd:string .

rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Module
  a rm:enumEntry ;
  rdf:value "Module"^^xsd:string ;
  dcterms:title "Модуль" ;
  dcterms:description "Содержит упорядоченный иерархический список элементов" ;
  owl:sameAs <http://jazz.net/ns/rm#Module> .

rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Collection
  a rm:enumEntry ;
  rdf:value "Collection"^^xsd:string ;
  dcterms:title "Набор" ;
  dcterms:description "Содержит неупорядоченный набор элементов" ;
  owl:sameAs <http://jazz.net/ns/rm#Collection> .

rmUserTypes:_YwcOsRmREemK5LEaKhoOow_Text
  a rm:enumEntry ;
  rdf:value "Text"^^xsd:string ;
  dcterms:title "Текст" ;
  dcterms:description "Содержит форматированный текст" ;
  owl:sameAs <http://jazz.net/ns/rm#Text> .

