@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix oslc:    <http://open-services.net/ns/core#> .
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix vann:    <http://purl.org/vocab/vann/> .
@prefix vs:      <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix sh:      <http://www.w3.org/ns/shacl#> .

@prefix rm:      <http://cpgu.kbpm.ru/ns/rm/rdf#> .
@prefix nav:     <http://cpgu.kbpm.ru/ns/rm/navigation#> .

# in context http://cpgu.kbpm.ru/ns/rm/rdf#

# The Requirements Management ontology
# Artifact переводим везде как Ресурс

rm:
  a owl:Ontology ;
  dcterms:title "The Requirements Management(RM) Vocabulary Extension" ;
  dcterms:description "All vocabulary URIs defined in the AL Requirements Management (RM) namespace."^^rdf:XMLLiteral ;
  vann:preferredNamespacePrefix "al_rm" ;
  rdfs:label "AL Requirements Management(RM)" .

# ******** Classes ********

rm:ArtifactType
  a                   rdfs:Class ;
  rdfs:isDefinedBy    rm: ;
  rdfs:label          "Тип ресурса" ;
  rdfs:comment        "Тип ресурса." .

rm:ArtifactTypeShape
  a                       sh:NodeShape ;
  sh:targetClass          rm:ArtifactType ;
  sh:property
    rm:identifierShape ,
    rm:titleShape ,
    rm:descriptionShape ,
    rm:creatorShape ,
    rm:createdShape ,
    rm:modifiedByShape ,
    rm:modifiedShape ,
    nav:processAreaShape ,
    rm:assetFolderShape .

rm:LinkType
  a                   rdfs:Class ;
  rdfs:isDefinedBy    rm: ;
  rdfs:label          "Тип связи" ;
  rdfs:comment        "Тип связи." .

#rm:enumEntries

rm:enumEntry
  a                   rdf:type ;
  rdfs:isDefinedBy    rm: ;
  rdfs:label          "Enumeration element" ;
  rdfs:comment        "Enumeration element." .

# ******** Properties ********

rm:identifierShape
  sh:name             "identifier" ;
  sh:path             dcterms:identifier ;
  dcterms:description """A service defined identifier."""^^rdf:XMLLiteral ;
  sh:minCount         1 ;
  sh:maxCount         1 ;
  sh:datatype         xsd:integer .

rm:titleShape
  sh:name             "title" ;
  sh:path             dcterms:title ;
  dcterms:description """A brief title for the resource. SHOULD include only content that is valid and suitable inside an XHTML <code>&lt;span&gt;</code> element."""^^rdf:XMLLiteral ;
  sh:minCount         0 ;
  sh:maxCount         1 ;
  sh:datatype         xsd:string .

rm:descriptionShape
  sh:name             "description" ;
  sh:path             dc:description ;
  dcterms:description """Details or body of the resource. SHOULD include only content that is valid and suitable inside an XHTML <code>&lt;div&gt;</code> element."""^^rdf:XMLLiteral ;
  sh:maxCount         1 ;
  sh:datatype         xsd:string .

rm:creatorShape
  sh:name             "creator"^^xsd:string ;
  sh:path             dcterms:creator ;
  dcterms:description """The person who created the resource."""^^rdf:XMLLiteral ;
  sh:minCount         1;
  sh:maxCount         1;
  sh:class            foaf:Person ;
  sh:nodeKind         sh:BlankNodeOrIRI .

rm:createdShape
  sh:name             "created"^^xsd:string ;
  sh:path             dcterms:created ;
  dcterms:description """When the resource was created."""^^rdf:XMLLiteral ;
  sh:minCount         1;
  sh:maxCount         1;
  sh:datatype         xsd:dateTime .

rm:modifiedByShape
  sh:name             "modifiedBy"^^xsd:string ;
  sh:path             dcterms:modifiedBy ;
  dcterms:description """The person who modified the resource."""^^rdf:XMLLiteral ;
  sh:minCount         1;
  sh:maxCount         1;
  sh:class            foaf:Person ;
  sh:nodeKind         sh:BlankNodeOrIRI .

rm:modifiedShape
  sh:name             "modified"^^xsd:string ;
  sh:path             dcterms:modified ;
  dcterms:description """When the resource was modified."""^^rdf:XMLLiteral ;
  sh:minCount         1;
  sh:maxCount         1;
  sh:datatype         xsd:dateTime .

