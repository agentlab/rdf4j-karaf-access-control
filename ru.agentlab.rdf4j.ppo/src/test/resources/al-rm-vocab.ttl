rm:artifactFormat
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Формат ресурса." ;
  rdfs:label           "Формат" .

rm:defaultFormat
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Формат ресурса по-умолчанию." ;
  rdfs:label           "Формат по-умолчанию" .

rm:inCreationMenu
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Показвается или нет тип ресурса в меню Создать." ;
  rdfs:label           "Показывать в меню Создать" .

rm:iconReference
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Ссылка на иконку." ;
  rdfs:label           "Иконка" .

rm:subjectToObjectLabel
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Описание объекта связи." ;
  rdfs:label           "Объект связи" .

rm:objectToSubjectLabel
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Описание субъекта связи." ;
  rdfs:label           "Субъект связи" .

rm:resourceContext
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Контекст." ;
  rdfs:label           "Контекст" .

rm:assetFolder
  a                    rdf:Property ;
  rdfs:isDefinedBy     rm: ;
  rdfs:comment         "Папка в проекте." ;
  rdfs:label           "Папка в проекте" .

rm:assetFolderShape
  sh:name             "assetFolder" ;
  sh:path             rm:assetFolder ;
  sh:minCount         1 ;
  sh:maxCount         1 ;
  sh:nodeKind         sh:IRI .

